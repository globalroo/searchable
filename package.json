{
	"name": "searchable",
	"version": "0.1.0",
	"private": false,
	"dependencies": {
		"@reach/router": "1.2.1",
		"react": "^16.8.4",
		"react-dom": "^16.8.4",
		"react-scripts": "2.1.8",
		"reset-css": "4.0.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"coverage": "react-scripts test --coverage",
		"codecov": "yarn test --coverage && codecov",
		"eject": "react-scripts eject"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/*.js",
			"src/**/*.js",
			"!src/serviceWorker.js",
			"!src/index.js",
			"!src/tmdb-service/tmdb-key.js"
		]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"codecov": "3.2.0",
		"husky": "1.3.1",
		"jest-fetch-mock": "2.1.1",
		"prettier": "1.16.4",
		"pretty-quick": "1.10.0",
		"react-testing-library": "6.0.0"
	}
}
